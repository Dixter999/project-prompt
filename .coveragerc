[run]
source = src
include = 
    src/utils/config.py
    src/utils/logger.py
omit = 
    */tests/*
    */test_*
    */__pycache__/*
    */venv/*
    */env/*
    */site-packages/*
    setup.py
    src/utils/mock_keyring.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

fail_under = 50
show_missing = True
skip_covered = False

[html]
directory = htmlcov